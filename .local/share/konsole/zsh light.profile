[Appearance]
ColorScheme=Arc
Font=Hack,11,-1,5,50,0,0,0,0,0,Regular

[General]
Command=/bin/zsh
Environment=TERM=xterm-256color,COLORTERM=truecolor,LIGHT_THEME=true
Name=zsh light
Parent=FALLBACK/
TerminalColumns=100
TerminalRows=29

[Interaction Options]
DropUrlsAsText=true
MiddleClickPasteMode=1
MouseWheelZoomEnabled=false
OpenLinksByDirectClickEnabled=true
UnderlineLinksEnabled=false
WordCharacters=-_

[Scrolling]
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=true
